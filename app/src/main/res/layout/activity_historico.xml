<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.erick.paradacerta.HistoricoActivity">


    <ListView
        android:id="@+id/listviewid"
        android:layout_width="334dp"
        android:layout_height="415dp"
        android:layout_marginTop="114dp"
        android:background="@color/cardview_shadow_start_color"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/BtnCadastro"
        android:text="@string/BtnCadastro"
        tools:text="@string/BtnCadastro"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="false"
        android:layout_alignParentTop="false"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.079"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_marginEnd="16dp"
        />

    <Button
        android:id="@+id/BtnMaps"
        android:layout_below="@+id/btn_show"
        android:text="@string/BtnMaps"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="false"
        android:layout_alignParentTop="false"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/BtnCadastro"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.079"
        app:layout_constraintHorizontal_bias="0.4"
        tools:ignore="RtlHardcoded"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="8dp"
        tools:text="Maps" />

</android.support.constraint.ConstraintLayout>
